{
  "source": ["tokens/**/*.json"],
  "platforms": {
    "css": {
      "transformGroup": "css",
      "files": [
        {
          "destination": "css/variables.css",
          "format": "css/variables"
        }
      ]
    },
    "scss": {
      "transformGroup": "scss",
      "files": [
        {
          "destination": "scss/variables.css",
          "format": "scss/variables"
        }
      ]
    },
    "flutter": {
      "transformGroup": "flutter",
      "files": [
        {
          "destination": "flutter/variables.dart",
          "format": "flutter/class.dart"
        }
      ]
    }
  }
}
